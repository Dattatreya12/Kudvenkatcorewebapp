@model int
@{
    ViewBag.Title = "404 Error";
}

<div class="alert alert-danger mt-1 mb-1">
    <h2>404 Not Found Error</h2>
    <hr />
    <h4>
        Employee with Id :@Model cannot be found
    </h4>

    <a asp-controller="home" asp-action="Index" class="btn btn-outline-success">BACK</a>
</div>

